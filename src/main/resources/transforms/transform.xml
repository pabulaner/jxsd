<?xml version="1.0" encoding="UTF-8" ?>
<transform pkg="drawingml.2006.chart">

    <!-- COMPONENT -->

    <interface name="IT_AxIdComponent">
        <method name="axId" type="CT_UnsignedInt" list="true" />
    </interface>
    <interface name="IT_BarGroupingComponent">
        <method name="grouping" type="CT_BarGrouping" />
    </interface>
    <interface name="IT_DropLinesComponent">
        <method name="dropLines" type="CT_ChartLines" />
    </interface>
    <interface name="IT_ErrBarsComponent">
        <method name="errBars" type="CT_ErrBars" />
    </interface>
    <interface name="IT_GapDepthComponent">
        <method name="gapDepth" type="CT_GapAmount" />
    </interface>
    <interface name="IT_GroupingComponent">
        <method name="grouping" type="CT_Grouping" />
    </interface>
    <interface name="IT_InvertIfNegativeComponent">
        <method name="invertIfNegative" type="CT_Boolean" />
    </interface>
    <interface name="IT_MarkerComponent">
        <method name="marker" type="CT_Marker" />
    </interface>
    <interface name="IT_PictureOptionsComponent">
        <method name="pictureOptions" type="CT_PictureOptions" />
    </interface>
    <interface name="IT_SmoothComponent">
        <method name="smooth" type="CT_Boolean" />
    </interface>
    <interface name="IT_TrendlineComponent">
        <method name="trendline" type="CT_Trendline" />
    </interface>

    <!-- CHART -->

    <interface name="IT_Chart">
        <generic name="TSer" extends="IT_Ser" />
        <method name="dLbls" type="CT_DLbls" />
        <method name="extLst" type="CT_ExtensionList" />
        <method name="ser" type="TSer" list="true" />
        <method name="varyColors" type="CT_Boolean" />
        <method name="builder" type="IT_ChartBuilder" />
    </interface>
    <interface name="IT_AreaChart">
        <extends name="IT_Chart">
            <generic name="CT_AreaSer" />
        </extends>
        <extends name="IT_AxIdComponent" />
        <extends name="IT_DropLinesComponent" />
        <extends name="IT_GroupingComponent" />
        <method name="dropLines" type="CT_ChartLines" />
    </interface>
    <interface name="IT_BarChart">
        <extends name="IT_Chart" />
        <extends name="IT_AxIdComponent" />
        <method name="barDir" type="CT_BarDir" />
        <method name="gapWidth" type="CT_GapAmount" />
    </interface>
    <interface name="IT_BubbleChart">
        <extends name="IT_Chart" />
        <method name="bubble3D" type="CT_Boolean" />
        <method name="bubbleScale" type="CT_BubbleScale" />
        <method name="showNegBubbles" type="CT_Boolean" />
        <method name="sizeRepresents" type="CT_SizeRepresents" />
    </interface>
    <interface name="IT_LineChart">
        <extends name="IT_Chart" />
    </interface>
    <interface name="IT_PieChart">
        <extends name="IT_Chart" />
        <method name="firstSliceAng" type="CT_FirstSliceAng" />
    </interface>
    <interface name="IT_ScatterChart">
        <extends name="IT_Chart" />
        <extends name="IT_AxIdComponent" />
        <method name="scatterStyle" type="CT_ScatterStyle" />
    </interface>

    <!-- SERIES -->

    <interface name="IT_Ser">
        <method name="idx" type="CT_UnsignedInt" />
        <method name="order" type="CT_UnsignedInt" />
        <method name="tx" type="CT_SerTx" />
        <method name="spPr" pkg="drawingml.main" type="CT_ShapeProperties" />
        <method name="dPt" type="CT_DPt" list="true" />
        <method name="dLbls" type="CT_DLbls" />
        <method name="cat" type="CT_AxDataSource" />
        <method name="val" type="CT_NumDataSource" />
        <method name="extLst" type="CT_ExtensionList" />
    </interface>

    <!-- CLASSES -->

    <class name="CT_AreaChart">
        <implements name="IT_AreaChart" />
    </class>
    <class name="CT_Area3DChart">
        <implements name="IT_AreaChart" />
        <implements name="IT_GapDepthComponent" />
    </class>
    <class name="CT_BarChart">
        <implements name="IT_BarChart" />
    </class>
    <class name="CT_Bar3DChart">
        <implements name="IT_BarChart" />
        <implements name="IT_GapDepthComponent" />
    </class>
    <class name="CT_BubbleChart">
        <implements name="IT_BubbleChart" />
    </class>
    <class name="CT_LineChart">
        <implements name="IT_LineChart" />
    </class>
    <class name="CT_Line3DChart">
        <implements name="IT_LineChart" />
        <implements name="IT_GapDepthComponent" />
    </class>
    <class name="CT_PieChart">
        <implements name="IT_PieChart" />
    </class>
    <class name="CT_DoughnutChart">
        <implements name="IT_PieChart" />
    </class>
    <class name="CT_ScatterChart">
        <implements name="IT_ScatterChart" />
    </class>

    <class name="CT_AreaSer">
        <implements name="IT_Ser" />
        <implements name="IT_PictureOptionsComponent" />
    </class>
    <class name="CT_BarSer">
        <implements name="IT_Ser" />
        <implements name="IT_InvertIfNegativeComponent" />
        <implements name="IT_PictureOptionsComponent" />
    </class>
    <class name="CT_BubbleSer">
        <implements name="IT_Ser" />
        <implements name="IT_InvertIfNegativeComponent" />
        <rename value="xVal" with="cat" />
        <rename value="yVal" with="val" />
    </class>
    <class name="CT_LineSer">
        <implements name="IT_Ser" />
        <implements name="IT_MarkerComponent" />
        <implements name="IT_SmoothComponent" />
    </class>
    <class name="CT_PieSer">
        <implements name="IT_Ser" />
    </class>
    <class name="CT_ScatterSer">
        <implements name="IT_Ser" />
        <implements name="IT_MarkerComponent" />
        <implements name="IT_SmoothComponent" />
        <rename value="xVal" with="cat" />
        <rename value="yVal" with="val" />
    </class>

    <class name="CT_PlotArea">
        <replace value="AreaChartOrArea3DChartOrLineChart" with="IT_Chart" />
        <rename value="areaChartOrArea3DChartOrLineChart" with="charts" />
    </class>
</transform>